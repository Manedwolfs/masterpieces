{
  "buildCommand": "api/src/index.js"
}
{
  "cleanUrls": true
}
{
  "crons": [
    {
      "path": "/api/every-minute",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/every-hour",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/every-day",
      "schedule": "0 0 * * *"
    }
  ]
}
{
  "devCommand": "next dev"
}
